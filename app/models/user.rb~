class User < ActiveRecord::Base
  #has_many :jobs, :transactions
  #has_one :profile
  
  def self.create_with_omniauth(auth)
   create! do |user|
     user.provider = auth["provider"]
     user.uid = auth["uid"]
     user.name = auth["info"]["name"]
     user.email = auth["info"]["email"]
   end
  
  end
end
