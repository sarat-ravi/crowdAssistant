<h1>Listing mobworkers</h1>

<table>
  <tr>
    <th>Api</th>
    <th>Assistant</th>
    <th>Rating</th>
    <th>Skill</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mobworkers.each do |mobworker| %>
  <tr>
    <td><%= mobworker.api_id %></td>
    <td><%= mobworker.assistant_id %></td>
    <td><%= mobworker.rating %></td>
    <td><%= mobworker.skill %></td>
    <td><%= link_to 'Show', mobworker %></td>
    <td><%= link_to 'Edit', edit_mobworker_path(mobworker) %></td>
    <td><%= link_to 'Destroy', mobworker, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mobworker', new_mobworker_path %>
