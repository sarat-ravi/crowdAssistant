<h1 id="heading1"><%= current_user.name %>'s Profile</h1>

<table class="table padded">
  <tr>
    <th>Age</th>
    <th>Gender</th>
    <th>Pic url</th>
    <th>Address</th>
    <th>Phone Number</th>
  </tr>
  <tr>
    <td><%= @profile.age %></td>
    <td><%= @profile.gender %></td>
    <td><%= @profile.pic_url %></td>
    <td><%= @profile.address %></td>
    <td><%= @profile.phonenum %></td>
  </tr>
</table>
<div class="rounded dark_shadow links padded">
<%= link_to 'Edit', edit_profile_path %>
</div>
<br />
